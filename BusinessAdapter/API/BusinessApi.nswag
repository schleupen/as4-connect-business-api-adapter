e\": {\r\n                  \"style\": \"form\"\r\n                },\r\n                \"Payload\": {\r\n                  \"style\": \"form\"\r\n                },\r\n                \"MessageId\": {\r\n                  \"style\": \"form\"\r\n                },\r\n                \"SenderMessageId\": {\r\n                  \"style\": \"form\"\r\n                },\r\n                \"BDEWDocumentType\": {\r\n                  \"style\": \"form\"\r\n                },\r\n                \"BDEWDocumentNo\": {\r\n                  \"style\": \"form\"\r\n                },\r\n                \"BDEWFulfillmentDate\": {\r\n                  \"style\": \"form\"\r\n                },\r\n                \"BDEWSubjectPartyId\": {\r\n                  \"style\": \"form\"\r\n                },\r\n                \"BDEWSubjectPartyRole\": {\r\n                  \"style\": \"form\"\r\n                }\r\n              }\r\n            }\r\n          }\r\n        },\r\n        \"responses\": {\r\n          \"202\": {\r\n            \"description\": \"Nachricht wurde erfolgreich empfangen.\",\r\n            \"content\": {\r\n              \"application/json\": {\r\n                \"schema\": {\r\n                  \"$ref\": \"#/components/schemas/SubmitFPMessageResponseDto\"\r\n                }\r\n              }\r\n            }\r\n          },\r\n          \"400\": {\r\n            \"description\": \"Nachricht wurde abgelehnt.\",\r\n            \"content\": {\r\n              \"application/json\": {\r\n                \"schema\": {\r\n                  \"$ref\": \"#/components/schemas/ProblemDetails\"\r\n                },\r\n                \"example\": {\r\n                  \"type\": \"https://tools.ietf.org/html/rfc9110#section-15.5.1\",\r\n                  \"title\": \"Bad Request\",\r\n                  \"status\": 400,\r\n                  \"detail\": \"Content validation failed.\"\r\n                }\r\n              }\r\n            }\r\n          },\r\n       